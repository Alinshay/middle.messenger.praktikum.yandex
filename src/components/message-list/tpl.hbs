{{#each data}}
    <div class="messages__date">{{date}}</div>
    {{#each messages}}
        <div>
            <div class="messages__item {{whose}}">
                <p class="item__text">{{content}}</p>
                <span class="item__time">{{time}}</span>
            </div>
        </div>
    {{/each}}
{{/each}}

